@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Hakkımda Veri Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Hakkımda Başlığı</th>
        <th>Durum</th>
        <th>Aktif Et</th>
        <th>Pasif Et</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>Başlık</td>
            <td>@item.AboutStatus</td>
            <td> <a href="/AdminAbout/EnableAbout/@item.AboutId" class="btn btn-warning">Aktif Et</a></td>
            <td> <a href="/AdminAbout/DisableAbout/@item.AboutId" class="btn btn-danger">Pasif Et</a></td>
        </tr>
    }
</table>
<br />
@Html.Action("AboutPartial","AdminAbout")
<br />